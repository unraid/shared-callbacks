{
  "name": "@unraid/shared-callbacks",
  "version": "3.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unraid/shared-callbacks"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:types && pnpm build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "tsup",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "prebuild": "pnpm clean",
    "test": "vitest --config vitest.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "crypto-js": "^4.2.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.0",
    "@vue/test-utils": "^2.4.6",
    "rimraf": "^6.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.0"
  },
  "packageManager": "pnpm@10.29.1+sha512.48dae233635a645768a3028d19545cacc1688639eeb1f3734e42d6d6b971afbf22aa1ac9af52a173d9c3a20c15857cfa400f19994d79a2f626fcc73fccda9bbc",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "vue-demi"
    ]
  }
}
